<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.yourwelcome.Home">


    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <Button
            android:id="@+id/inlineID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Inline" />

        <Button
            android:id="@+id/interfaceID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Interface" />

        <Button
            android:id="@+id/returnID"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="Volver"
            android:text="Volver" />

        <Button
            android:id="@+id/ir"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="irPulso"
            android:text="Sumar" />
        <Button
            android:id="@+id/irServicio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="irServicio"
            android:text="Servicio" />

        <Button
            android:id="@+id/button4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="IrFrag"
            android:text="Fragmento" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
